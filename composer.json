{
	"name": "seppzzz/silverstripe-elemental-linkify",
	"description": "A Silverstripe module to create links to elemental elements.",
	"type": "silverstripe-vendormodule",
	"license": "BSD-3-Clause",
	"keywords": [
		"silverstripe",
		"CMS",
		"elemental",
		"linking"
	],
	"require": {
		"php": "^8.1",
		"silverstripe/framework": "^6.0",
		"silverstripe/cms": "^6.0",
		"silverstripe/admin": "^3.0",
		"silverstripe/asset-admin": "^3.0",
		"silverstripe/htmleditor": "^3.0",
		"silverstripe/vendor-plugin": "^2.0"
	},
	"autoload": {
		"psr-4": {
			"Zazama\\ElementalLinkify\\": "src/"
		}
	},
	"extra": {
		"expose": [
			"client/dist/js"
		]
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}

